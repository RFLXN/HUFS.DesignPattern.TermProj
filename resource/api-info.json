{
  "apiRoot": "https://www.virustotal.com/api/v3",
  "endpoints": {
    "files": {
      "report": {
        "path": "/files/{id}",
        "method": "GET",
        "pathParams": [
          "{id}"
        ]
      },
      "scan": {
        "path": "/files",
        "method": "POST"
      }
    },
    "analysis": {
      "analysis": {
        "path": "/analyses/{id}",
        "method": "GET",
        "pathParams": [
          "{id}"
        ]
      }
    },
    "url": {
      "scan": {
        "path": "/urls",
        "method": "POST"
      },
      "report": {
        "path": "/urls/{id}",
        "method": "GET",
        "pathParams": [
          "{id}"
        ]
      }
    }
  }
}